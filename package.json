{
  "name": "leexdoc",
  "version": "0.1.0",
  "private": true,
  "author": "Ahmet Tural",
  "description": "Document management and storage system",
  "scripts": {
    "dev": "node ./bin/start-dev.mjs",
    "dev:api": "cross-env NEXT_TELEMETRY_DISABLED=1 NODE_ENV=development NODE_OPTIONS=\"--trace-warnings\" DEBUG=prisma:* next dev -p ${PORT:-3000}",
    "dev:worker": "ts-node src/worker/transcode.ts",
    "diagnose": "node ./bin/diagnose.mjs",
    "prisma:logs": "cross-env DEBUG=prisma:* prisma generate",
    "build": "next build",
    "start": "next start",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed": "cd prisma && ts-node seed.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:ci": "npm run test:run -- --reporter=json --outputFile=test-results.json && npm run test:e2e -- --reporter=json --outputFile=e2e-results.json",
    "docker:dev": "docker compose --profile dev up -d",
    "docker:prod": "docker compose --profile prod up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "init:minio": "bash scripts/init-minio.sh",
    "create:admin": "node scripts/create-admin.js",
    "validate": "node scripts/validate-system-simple.js",
    "validate:full": "node scripts/validate-system.js",
    "setup": "cp .env.example .env && echo 'Please edit .env file with your configuration'",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.886.0",
    "@aws-sdk/s3-request-presigner": "^3.886.0",
    "@prisma/client": "^6.16.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@types/jsonwebtoken": "^9.0.10",
    "argon2": "^0.44.0",
    "axios": "^1.12.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.58.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "dotenv-expand": "^12.0.3",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.4",
    "idb": "^8.0.3",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.543.0",
    "music-metadata": "^11.8.3",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "next-pwa": "^5.6.0",
    "node-id3": "^0.2.9",
    "pino": "^9.9.5",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.16.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1",
    "workbox-window": "^7.3.0",
    "zod": "^4.1.7",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^10.0.0",
    "cross-spawn": "^7.0.6",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "execa": "^9.6.0",
    "jsdom": "^26.1.0",
    "node-fetch": "^2.7.0",
    "tailwindcss": "^4",
    "tcp-port-used": "^1.0.2",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
